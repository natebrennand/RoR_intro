<h1>Listing lectures</h1>

<table>
  <tr>
    <th>Class time</th>
    <th>Subject</th>
    <th>Attendance</th>
    <th>Belongs to</th>
    <th>Embeds many</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lectures.each do |lecture| %>
  <tr>
    <td><%= lecture.class_time %></td>
    <td><%= lecture.subject %></td>
    <td><%= lecture.attendance %></td>
    <td><%= lecture.belongs_to %></td>
    <td><%= lecture.embeds_many %></td>
    <td><%= link_to 'Show', lecture %></td>
    <td><%= link_to 'Edit', edit_lecture_path(lecture) %></td>
    <td><%= link_to 'Destroy', lecture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Lecture', new_lecture_path %>
